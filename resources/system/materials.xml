<materialslib>

	<material name="ambient" bumped="false" skinned="false">
		<pass type="Z_PRE_PASS" vertex="xyzuvn" shader="z_pre_pass"/>
		<pass type="MAIN_COLOR" vertex="xyzuvn" shader="ambient"/>
	</material>
  
	<material name ="diffuse" bumped="false" skinned="false">
		<pass type="Z_PRE_PASS" vertex="xyzuvn" shader="z_pre_pass"/>
		<pass type="MAIN_COLOR" vertex="xyzuvn" shader="diffuse"/>
	</material>

	<material name="bump" bumped="true" skinned="false">
		<pass type="Z_PRE_PASS" vertex="xyzuvntb" shader="z_pre_pass" pins="PIN_BUMP_MAP"/>
		<pass type="MAIN_COLOR" vertex="xyzuvntb" shader="diffuse" pins="PIN_BUMP_MAP"/>
	</material>

	<material name="bump_alpha_test" bumped="true" skinned="false">
		<pass type="Z_PRE_PASS" vertex="xyzuvntb" shader="z_pre_pass" pins="PIN_BUMP_MAP|PIN_ALPHA_TEST"/>
		<pass type="MAIN_COLOR" vertex="xyzuvntb" shader="diffuse" pins="PIN_BUMP_MAP|PIN_ALPHA_TEST"/>
	</material>

	<material name="diffuse_skinned" bumped="false" skinned="true">
		<pass type="Z_PRE_PASS" vertex="nuv2ui" shader="skinned_z_pre_pass"/>
		<pass type="MAIN_COLOR" vertex="nuv2ui" shader="diffuse_skinned"/>
	</material>
 
</materialslib>